          "rawQuery": true,
          "rawSql": "\r\nSELECT \r\n  now() as time_sec,\r\n  DATE_FORMAT(temp3.x_date,'%m.%d'),\r\n  case\r\n  when sum(temp3.cnt) is null then 0\r\n  else temp3.cnt\r\n  end as ''\r\nfrom (\r\nselect temp.x_date, temp2.cnt from (select \r\nx_date\r\nfrom (\r\n    select t.*, t.date1 + INTERVAL (a.a + (10 * b.a) + (100 * c.a)) DAY as x_date\r\n    from (select 0 as a union all select 1 union all select 2 union all select 3 union all select 4 union all select 5 union all select 6 union all select 7 union all select 8 union all select 9) as a\r\n    cross join (select 0 as a union all select 1 union all select 2 union all select 3 union all select 4 union all select 5 union all select 6 union all select 7 union all select 8 union all select 9) as b\r\n    cross join (select 0 as a union all select 1 union all select 2 union all select 3 union all select 4 union all select 5 union all select 6 union all select 7 union all select 8 union all select 9) as c\r\n    join (\r\n        select begin as date1, end as date2 from zt_project where id=${sprint_id}\r\n    ) t\r\n    where t.date1 + INTERVAL (a.a + (10 * b.a) + (100 * c.a)) DAY <= t.date2\r\n) a) temp left join (select SUBSTR(openedDate,1,10) as date_time, count(*) as cnt from zt_bug where deleted='0' and project=${sprint_id} GROUP BY SUBSTR(openedDate,1,10) ORDER BY date_time asc) temp2 on temp.x_date=temp2.date_time ORDER BY temp.x_date asc) temp3\r\nGROUP BY temp3.x_date;",
